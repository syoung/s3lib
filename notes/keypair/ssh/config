# annai EC2
host *.amazonaws.com
 User root
 StrictHostKeyChecking no
 IdentityFile ~/.ssh/annai/ec2/id_rsa
 UserKnownHostsFile ~/.ssh/annai/ec2/known_hosts

# annai GITHUB
Host github.com
 User annaisystems
 HostName github.com
 PreferredAuthentications publickey
 IdentityFile ~/.ssh/annaisystems/github/id_rsa
 
# annaisystems BITBUCKET
Host bitbucket.org
 User annaisystems
 HostName bitbucket.org
 PreferredAuthentications publickey
 IdentityFile ~/.ssh/annaisystems/bitbucket/id_rsa
