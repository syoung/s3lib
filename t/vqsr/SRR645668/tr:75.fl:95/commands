1: 
java -jar /agua/apps/gatk/3.2/GenomeAnalysisTK.jar -T VariantRecalibrator -R /data/reference/genome.fa -input /data/nrc/SRR645668/SRR645668_GVCF.vcf -resource:omni,known=false,training=true,truth=true,prior=12.0 /agua/apps/gatk/3.2/ftp.broadinstitute.org/bundle/2.5/b37/1000G_omni2.5.b37.vcf -resource:hapmap,known=false,training=true,truth=true,prior=15.0 /agua/apps/gatk/3.2/ftp.broadinstitute.org/bundle/2.5/b37/hapmap_3.3.b37.vcf -an QD -an FS -an MQRankSum -an ReadPosRankSum -mode SNP -tranche 100.0 -tranche 99.9 -tranche 99.0 -tranche 90.0 -tranche 75.0-recalFile /data/nrc/SRR645668_recalibrated_SNP.recal -tranchesFile /data/nrc/SRR645668_recalibrated_SNP.tranches -rscriptFile /data/nrc/SRR645668_recalibrated_SNP_plots.R
2:
java -jar /agua/apps/gatk/3.2/GenomeAnalysisTK.jar -T ApplyRecalibration -R /data/reference/genome.fa -input /data/nrc/SRR645668/SRR645668_GVCF.vcf -mode SNP --ts_filter_level 95.0 -recalFile /data/nrc/SRR645668_recalibrated_SNP.recal -tranchesFile /data/nrc/SRR645668_recalibrated_SNP.tranches -o /data/nrc/SRR645668_recalibratedsnps_raw_indels.vcf
3:
java -jar /agua/apps/gatk/3.2/GenomeAnalysisTK.jar -T VariantRecalibrator -R /data/reference/genome.fa -input /data/nrc/SRR645668_recalibratedsnps_raw_indels.vcf -resource:mills,known=true,training=true,truth=true,prior=12.0 /data/reference/varscan/Mills_and_1000G_gold_standard.indels.b37.vcf -an FS -an MQRankSum -an ReadPosRankSum -mode INDEL -tranche 100.0 -tranche 99.9 -tranche 99.0 -tranche 90.0 tranche 75.0 --maxGaussians 4 -recalFile /data/nrc/SRR645668_recalibrated_INDEL.recal -tranchesFile /data/nrc/SRR645668_recalibrated_INDEL.tranches -rscriptFile /data/nrc/SRR645668_recalibrated_INDEL_plots.R
4:
java -jar /agua/apps/gatk/3.2/GenomeAnalysisTK.jar -T ApplyRecalibration -R /data/reference/genome.fa -input /data/nrc/SRR645668_recalibratedsnps_raw_indels.vcf -mode INDEL --ts_filter_level 95.0 -recalFile /data/nrc/SRR645668_recalibrated_INDEL.recal -tranchesFile /data/nrc/SRR645668_recalibrated_INDEL.tranches -o /data/nrc/SRR645668_recalibrated_variants.vcf
